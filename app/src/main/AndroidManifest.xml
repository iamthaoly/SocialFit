<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iamthaoly.fitapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/fit"
        android:label="FitApp"
        android:roundIcon="@drawable/fit"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LogActivity"></activity>
        <activity android:name=".TrackerActivity" />
        <activity
            android:name=".TrackActivity"
            android:label="@string/title_activity_track"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ChallengeActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".StartActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>